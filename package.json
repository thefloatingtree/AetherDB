{
  "name": "typescript-esm-tsx-starter",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cron:elo-update:dev": "tsx src/daemons/cron/elo-update/index.ts",
    "cron:elo-update:dev:prod": "dotenv -e .env.prod -- tsx src/daemons/cron/elo-update/index.ts",
    "cron:elo-update:start": "node ./dist/daemons/cron/elo-update/index.js",
    "init": "pnpm install && pnpm run build",
    "build": "rimraf ./dist && rimraf ./src/generated && prisma generate && tsc && tsc-alias",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx)\"",
    "prisma:prod": "dotenv -e .env.prod -- prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@types/pg": "^8.11.10",
    "axios": "^1.7.9",
    "fast-xml-parser": "^4.5.1",
    "kysely": "^0.27.5",
    "pg": "^8.13.1",
    "prisma-kysely": "^1.8.0",
    "radash": "^12.1.0"
  }
}
